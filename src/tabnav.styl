.tabnav
  bb $b
  /.container-preview &-tabs
    ml 5%
  &-extra, &-tab
    c fg("faded")
  &-pr &-tab
    &.selected, &[aria-selected="true"], &[aria-current]
      c 0 0 base(2)
  &-tabs
    &[aria-label="Stargazers"] ^[0]-tab
      &:not(.selected)
        filter saturate(0.6)
      &.selected
        c 0 0 base(2)
    &.js-file-editor-nav ^[0]-tab
      c fg("faded") base(1) base(2)
      mt 8px
      rt 6px
  &--responsive &-tab, &-tab
    c fg("faded") base(1)
    &.selected, &[aria-selected="true"], &[aria-current]
      c fg("primary") base(1) base(5)
      &.js-blob-edit-
        &code, &preview
          c $blue base(1) base(5)
