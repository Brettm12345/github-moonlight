.commit
  &-loader .loader-error
    c $red
  &-link
    c $yellow
  &-author-section
    c $blue
  &-desc
    &+^[0]-branches
      btc $base1
    pre
      c $text 0 $base4
  &-icon .octicon, &-group-title .octicon-git-commit
    c $text 0 $base4
  &s-list-item
    &, &:hover
      c 0 0 $t
    &+&
      btc $base1
  &-ref
    c $text $base1 $base2
    fn $monoFont
    fw 500
    .user
      c $blue
  &-sha
    c $yellow $base1 $base5
  & &-title .issue-link
    c $blue
  & &-title, &-group-title, a
    c $text
  &-tease
    c $blue $base1 $base4
    &-sha
      c $blue
    &-contributors
      bt $b
      c 0 0 $base9
.commits
  blockquote
    c $text
  &-listing:before
    c 0 0 $base5
  &-list-item
    c 0 0 $base4
    &+&
      btc $base1
    &.navigation-focus
      c 0 0 $base5
    .commit-
      &desc pre, &author
        c $faded
.full-commit
  c $blue $base1 $base4
  .branches-list li
    c $textGray
    &.loading, &.pull-request
      c $faded
  .btn-outline
    &:not(:disabled):hover
      c $blue $blue
    &, &:disabled
      c 0 $base1
  p.commit-title
    c $blue
  .sha-block
    c $faded
    fs 14px
    fw 500
    & > a
      c $text $borderAlt
      &:hover
        c 0 $base1
    & > .sha
      c $blue
  .commit-meta
    c 0 $base1 $base5
.file-commit-form--full
  c $text $base1 $base2
.commit-form
  c 0 0 $base4
  caret "right"
.line-comments
  c 0 0 $base5
  /.diff-table tr:not(:last-child) &
    c 0 $base1
  /.empty-cell + &, & + &
    blc $base1
div.commit-build-statuses, .link-gray
  c $faded
  &:hover
    c $blue
  &-dark, body &-dark
    c $blue
    &:hover, body &:hover
      td underline
-expander
  .ellipsis&, .hidden-text& a
    c $text 0 $base7
