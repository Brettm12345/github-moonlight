code .link-gray
  set-font "mono"
a
  ease color
:not(.commit-build-statuses) > a:not(.IssueLabel):not(.IssueLabel--big):not(.lh-condensed-ultra):not(.module-linker):not(.tabnav-tab):not(.reponav-item):not(.btn)
  c fg("primary")
  &:hover
    c $blue
.muted-link
  c fg("faded")
  &:hover
    c $blue
.link-gray-dark
  c $blue
  octicon fg("dark")
  &:hover
    c light("blue")
.link-mktg
  shadow 0 1px 0 0 $blue
  &:hover
    shadow 0 1px 0 0 light("blue")
