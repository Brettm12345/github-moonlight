.graph-filter .info
  c fg("primary")
line[stroke="#000"]
  vc 0 base(7)
text
  &, &[fill="#000"], &[fill="#6a737d"]
    vc fg("primary")
.viz
  .code-frequency
    ../, &
      c fg("primary") 1 4
    p 8px
.c
  &add
    c v("green")
  &del
    c v("red")
.graphs
  .path
    vc 0 v("teal")
  .dot
    vc v("teal") v("teal")
  &.axis
    c 0 1
  .dir
    c fg("primary")
  .area
    vc v("teal")
.code-frequency
  .addition
    vc v("green")
  .deletion
    vc v("red")
.community-checklist .progress
  c 0 0 8
table.capped-list
  td
    c fg("primary") 1 4
  tr:nth-child(2n), th
    c fg("primary") 1 4
.summary-stats li
  &, & > .num, & > a
    ease background-color
  .octicon-git-branch
    c v("green")
  a
    ease background-color, color
  &:hover, &:focus
    &, & > .num, & > a
      c fg("light") 1 5
  /.traffic-graph-stats, &, & > .num
    c fg("primary") 1 4
.contrib-person path
  vc v("orange")
#contributors-master svg.viz
  c 0 0 4
.rule
  bb $b
.community-checklist .progress-bar
  bg linear-gradient(90deg,v("yellow"),v("orange"),v("green"))
