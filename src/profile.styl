.github-jobs-logo strong
  inv()
.pinned-item-
  &list-item &handle, &name
    c fg("faded")
  &checkbox:checked + &name
    c fg("primary") base(1) base(7)
.profile-rollup-
  &summarized
    &+&
      c 0 base(6)
  &wrapper
    &+&
      bt 1px solid base(5)
.activity-overview-
  &percentage
    vc fg("light")
  &label
    vc fg("faded")
.user-profile-
  &nav
    b $b
    px 10px
    r 8px
  &sticky-bar:after
    c 0 base(1) base(2)
.vcard-
  &username
    c fg("gray")
  &detail
    octicon fg("faded")
.js-profile-editable-edit-button
  c fg("light") base(1) base(5)
