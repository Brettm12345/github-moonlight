.branch-infobar
  c fg("placeholder") base(1) base(4)
.social-count
  c $blue base(1) base(5)
  &:hover
    c fg("light")
#readme
  .Box-header
    ease background-color
    c 0 base(1) base(3)
    o 1
    &:hover
      c 0 0 base(2)
    .octicon-pencil
      c $indigo
      ease color
      ../:hover ^[2..-1]
        c $blue
table
  &.files
    c fg("primary") base(1) base(4)
    td
      c 0 base(1)
      &.age
        c fg("primary")
      &.icon
        c fg("primary")
    tr
      ease background-color
    tr.navigation-focus, tr[aria-selected=true]
      td
        c 0 0 base(5)
.js-recent-activity-container > div > div
  c 0 0 base(2)
.repository-content > div.bg-white:first-child
  c 0 0 base(2)
.flash
  c fg("light") base(1) base(4)
  &-warn
    c $yellow
.num
  c fg("secondary")
