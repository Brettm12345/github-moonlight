.branch-infobar
  c fg("placeholder") 1 4
.num
  c fg("secondary")
.js-recent-activity-container > div > div,
.repository-content > div.bg-white:first-child
  c 0 0 2
#readme
  .Box-header
    ease colors
    c 0 1 3
    o 1
    hover 0 0 2
    .octicon-pencil
      c v("indigo")
      ease colors
      +parent-hover()
        c v("blue")
table
  &.files
    c fg("primary") 1 4
    tr
      ease colors
      +nav-focus()
        td
          c 0 0 6
      td
        c 0 1
        &.message
          c fg("faded")
        &.age, &.icon
          c fg("dark")
      &.up-tree
        c 0 1
        a
          b 1px solid $t
          ease colors
          &:hover, &:focus
            b $b
            c v("blue") 1 2
            td none
.hide-files-
  &btn
    c fg("secondary")
  &list a
    c fg("dark")
.btn[aria-label*="release"]
  c 0 0 5
