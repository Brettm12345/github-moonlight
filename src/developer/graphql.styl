iframe#graphiql
  &, .graphiql-ide
    c 0 base(1)
.graphiql-ide
  btc base(1)
.CodeMirror
  &-cursor
    blc $blue
  &-hint
    c fg("primary") base(1) base(4)
    ease background-color
    &-information
      c $indigo base(1) base(4)
    &-active
      c fg("light") 0 base(8)
      fw 500
  &-linenumber
    c $indigo
    fw 500
  &-gutters, &-gutter&-foldgutter
    c fg("primary") base(1) base(4)
  &.cm-s-graphiql
    c 0 0 base(4)
.history-, .doc-explorer-
  &contents
    c fg("primary") base(1) base(2)
  &title-bar
    c fg("primary") base(1) base(4)
.graphiql-container
  & .history-contents p
    c fg("primary") base(1)
  .execute-options > li,
  .toolbar-menu-items > li,
  .toolbar-select-options > li,
  .history-contents > p
    &:hover, &.hover, &:active
      c 0 base(1) base(8)
  .search-box
    c 0 base(1) base(2)
    input
      c 0 0 base(2)
  .result-window .CodeMirror-gutters
    c 0 base(1)
  .doc-category-title
    c fg("faded") base(1)
  .resultWrap
    blc base(1)
  .arg-name
    c $pink
  .field-name
    c $blue
  .keyword
    c $purple
  .type-name
    c $yellow
  .field-short-description
    c $indigo
  .topBar
    bg base(4)
    c fg("primary") base(1) base(4)
  .toolbar, .execute
    &-button
      bg base(2)
      shadow none
      svg
        vc fg("primary")
  .variable-editor-title
    c fg("primary") base(1) base(5)
  .docExplorerShow
    bg base(5)
    c fg("primary") base(1) base(5)
.result-window .CodeMirror
  c 0 0 base(4)
