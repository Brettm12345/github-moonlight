.filter-bar
  bb 2px solid base(1)
  c 0 0 2
header #search-results-container
  c 0 0 4
.search_repos
  fieldset
    c 0 0 3
    px 16px
.user, .issue
  &-list-item
    &+&
      btc base(1)
.code-list
  .file-box, & &-item + &-item, .divider .blob-code, .divider .blob-num, &-item-private .blob-num
    c fg("primary") 1
  &-item
    &.hx_hit-code&-public.repo-specific
      c 0 0 2
.jump-to-
  &field-active, /.js-navigation-item:hover, /#jump-to-results:hover
    c 0 0 5
  &suggestions
    &-results-container
      .navigation-
        &focus ^[1]-path
          c 0 0 8
        &item
          c 0 1
.ais-
  /.search-result-intro
    & mark, & .ais-Highlight-highlighted
      bbc v("blue")
  &Hits-item
    &:hover
      c 0 0 7
  &SearchBox-input
    bg base(4) url("/assets/images/octicons/search.svg") no-repeat 6px
    c fg("light") 1
